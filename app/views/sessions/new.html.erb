<div class="container">
  <%= form_with url: sign_in_path , local: true do |form| %>
    <div class="form-group">
	    <%= form.label :email %>
			<%= form.text_field :email, class: "form-control" %>
    </div>
    <div class="form-group">
	    <%= form.label :password %>
			<%= form.password_field :password, class: "form-control" %>
    </div>	
    <%= form.submit 'Sign in', class: "btn btn-default" %>
  </form>
	<% end %> 
</div>